# - Config file for the bullet package
# It defines the following variables
#  BULLET_INCLUDE_DIRS - include directories for osgOcean
#  BULLET_LIBRARIES    - libraries to link against

@PACKAGE_INIT@

set(BULLET_PREFIX "@CMAKE_INSTALL_PREFIX@")
set(BULLET_LIBRARIES "${BULLET_PREFIX}/lib/@CMAKE_PROJECT_NAME@/libBulletCollision.a;${BULLET_PREFIX}/lib/@CMAKE_PROJECT_NAME@/libBulletDynamics.a;${BULLET_PREFIX}/lib/@CMAKE_PROJECT_NAME@/libBulletSoftBody.a;${BULLET_PREFIX}/lib/@CMAKE_PROJECT_NAME@/libLinearMath.a" )

# Compute paths
set_and_check(BULLET_INCLUDE_DIRS "${BULLET_PREFIX}/include/@CMAKE_PROJECT_NAME@")
